{
  "name":"color_em",
  "description":"Change the colors of the URLs on the webpage",
  "version":"1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon":"icon.png",
    "default_title": "color_em",
    "default_popup": "popup.html" 
   },
  "permissions": [
    "tabs", "<all_urls>"
   ]
  
}